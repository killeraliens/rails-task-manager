<div class="container">
  <h1>GET'r DONE</h1>
  <form action= "/tasks"></form>
  <% @tasks.each do |task| %>
    <ul>
      <li>
        <div class="d-flex">
          <% if task.completed %>
            <i class="far fa-check-square"></i>
            <%  else %>
            <i class="far fa-square"></i>
            <% end %>
          <h3><%= task.title %></h3>
        </div>
      </li>
    </ul>
  <% end %>
</div>
